prefix=/home/veperez/Documents/CADRE/venv/lib/python2.7/site-packages/petsc
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${prefix}/lib
ccompiler=/usr/bin/mpicc
cflags_extra=-fPIC  -Wall -Wwrite-strings -Wno-strict-aliasing -Wno-unknown-pragmas -fstack-protector -fvisibility=hidden -g -O
